<?xml version="1.0" encoding="utf-8"?>

<xtic-aptitudes>
	<aptitude id="touch_languages" desc="Touch Programming Language files">
		<pattern id="java" desc="Touch Java files">
			<file value="\.java" />
		</pattern>
		<pattern id="c" desc="Touch C files">
			<file value="\.c" />
		</pattern>
		<pattern id="ruby" desc="Touch Ruby files">
			<file value="\.rb" />
		</pattern>
		<pattern id="python" desc="Touch Python files">
			<file value="\.py" />
		</pattern>
	</aptitude>
</xtic-aptitudes>
